{
    "name": "basic-app/appstarter",
    "type": "project",
    "description": "Basic App",
    "homepage": "http://basic-app.com",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2",
        "codeigniter4/framework": "*",
        "basic-app/admin": "*",
        "basic-app/module-admin-simple": "*",
        "basic-app/theme-bootstrap4": "*",
        "basic-app/theme-cool-admin": "*",
        "basic-app/system": "dev-master",
        "denis303/codeigniter4-migration": "dev-master",
        "denis303/codeigniter4-form": "dev-master",
        "denis303/php-file-helper": "dev-master",
        "denis303/php-download-helper": "dev-master",
        "denis303/php-zip-helper": "dev-master",
        "basic-app/crud": "dev-master",
        "basic-app/module-user-provider": "dev-master",
        "basic-app/module-user": "dev-master"
    },
    "require-dev": {
        "mikey179/vfsstream": "1.6.*",
        "phpunit/phpunit": "^7.0"
    },
    "scripts": {
        "post-update-cmd": [
            "@composer dump-autoload"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }        
    ]
}
